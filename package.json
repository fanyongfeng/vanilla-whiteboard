{
  "name": "vanilla-whiteboard",
  "version": "0.1.0",
  "description": "whiteboard by plain javascript",
  "author": "",
  "private": true,
  "typings": "./src/typings/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test ava --verbose #&& rm -rf ./coverage && mkdir ./coverage && nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "test:watch": "NODE_ENV=test ava --verbose --watch",
    "lint": "eslint src --ext '.js'",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "analy": "npm_config_report=true node build/build.js",
    "start": "rollup config rollup.config.js -c -w --environment NODE_ENV:development",
    "build": "rollup config rollup.config.js -c -w --environment NODE_ENV:production",
    "prettier": "prettier --write",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}"
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^2.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "autoprefixer": "^7.1.2",
    "ava": "next",
    "babel-eslint": "latest",
    "babel-loader": "^8.0.0",
    "babel-minify": "^0.4.3",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chalk": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "esm": "^3.0.81",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "next",
    "lint-staged": "^7.2.2",
    "node-notifier": "^5.1.2",
    "onchange": "^4.1.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "prettier": "1.14.2",
    "rimraf": "^2.6.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-uglify": "^5.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "typescript": "^3.0.3",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "ava": {
    "files": [
      "test/point.js"
    ],
    "require": [
      "./test/register.js",
      "esm"
    ],
    "serial": true
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "npm": "^6.4.1"
  }
}
